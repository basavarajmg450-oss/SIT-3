{
  "buildCommand": "cd placementpro-frontend && npm install && npm run build",
  "outputDirectory": "placementpro-frontend/dist",
  "framework": "vite",
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
